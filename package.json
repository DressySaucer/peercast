{
    "name": "peercast",
    "productName": "Peercast",
    "bundleID": "com.peercast.Peercast",
    "version": "0.3.0",
    "description": "Electron-based remote desktop application",
    "main": "dist/electron/main.js",
    "scripts": {
        "start": "electron-forge start",
        "build:dev": "webpack --config webpack.dev.ts && tsc && node-gyp rebuild && mkdir dist/bin && cp build/Release/vinput.node dist/bin/",
        "build:prod": "webpack --config webpack.prod.ts && tsc && node-gyp rebuild && mkdir dist/bin && cp build/Release/vinput.node dist/bin/",
        "serve": "ts-node scripts/serve.ts",
        "package": "electron-forge package",
        "make": "electron-forge make"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DressySaucer/peercast.git"
    },
    "author": "James Hardy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DressySaucer/peercast/issues"
    },
    "homepage": "https://github.com/DressySaucer/peercast#readme",
    "devDependencies": {
        "@electron-forge/cli": "^6.4.2",
        "@electron-forge/maker-dmg": "^7.1.0",
        "@electron-forge/maker-zip": "^6.4.2",
        "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
        "@electron/rebuild": "^3.3.1",
        "@react-oauth/google": "^0.11.1",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "css-loader": "^6.8.1",
        "electron": "^27.0.3",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "express": "^4.18.2",
        "html-webpack-plugin": "^5.5.3",
        "node-addon-api": "^7.0.0",
        "node-gyp": "^10.0.1",
        "prettier": "^3.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.18.0",
        "react-router-dom": "^6.18.0",
        "socket.io": "^4.7.2",
        "socket.io-client": "^4.7.2",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-middleware": "^6.1.1",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.10.0"
    }
}
